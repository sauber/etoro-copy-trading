{
  "lock": false,
  "tasks": {
    "build": "deno run -A dev.ts build",
    "check": "deno fmt --check && deno lint && deno check **/*.ts && deno check **/*.tsx",
    "community": "deno run --allow-read=testdata,../etoro-data/data src/repository/community_bin.ts ../etoro-data/data",
    "cli": "echo \"import '\\$fresh/src/dev/cli.ts'\" | deno run --unstable -A -",
    "manifest": "deno task cli manifest $(pwd)",
    "preview": "deno run -A main.ts",
    "profile": "deno run --allow-read=testdata,../etoro-data/data --v8-flags=--prof src/repository/community_bin.ts ../etoro-data/data",
    "rank": "deno run --allow-read=testdata,../etoro-data/data --allow-write=testdata,../etoro-data/data --allow-net=github.com,objects.githubusercontent.com src/ranking/ranking_bin.ts",
    "scrape": "deno run --allow-read=../etoro-data/data --allow-write=/workspaces/etoro-data/data --allow-net=www.etoro.com src/repository/refresh_bin.ts ../etoro-data/data",
    "start": "deno run -A --watch=web/static/,web/routes/,web/islands/ web/dev.ts",
    "test": "deno test --allow-write=tmp,testdata --allow-read=tmp,testdata --allow-net=www.etoro.com,jsonplaceholder.typicode.com,github.com,objects.githubusercontent.com",
    "update": "deno run -A -r https://fresh.deno.dev/update .",
    "webtest": "deno test --allow-env --allow-read=. --allow-net=127.0.0.1"
  },
  "lint": { "rules": { "tags": ["fresh", "recommended"] } },
  "nodeModulesDir": true,
  "imports": {
    "$fresh/": "https://deno.land/x/fresh@1.6.5/",
    "$fresh_charts/": "https://deno.land/x/fresh_charts@0.3.1/",
    "$std/": "https://deno.land/std@0.211.0/",
    "./": "./",
    "/": "./src/",
    "@preact/signals": "https://esm.sh/*@preact/signals@1.2.1",
    "@preact/signals-core": "https://esm.sh/*@preact/signals-core@1.5.0",
    "dataframe": "jsr:@sauber/dataframe@^0.0.5",
    "nanoid": "https://deno.land/x/nanoid@v3.0.0/mod.ts",
    "netsaur": "https://deno.land/x/netsaur@0.2.14/mod.ts",
    "preact": "https://esm.sh/preact@10.19.2",
    "preact-render-to-string": "https://esm.sh/*preact-render-to-string@6.2.2",
    "preact/": "https://esm.sh/preact@10.19.2/",
    "semaphore": "https://deno.land/x/semaphore@v1.1.2/semaphore.ts",
    "table": "jsr:@sauber/table@^0.0.3",
    "tailwindcss": "npm:tailwindcss@3.4.1",
    "tailwindcss/": "npm:/tailwindcss@3.4.1/",
    "tailwindcss/plugin": "npm:/tailwindcss@3.4.1/plugin.js",
    "$std/": "https://deno.land/std@0.211.0/",
    "chart": "https://deno.land/x/chart@1.1/mod.ts",
    "nanoid": "https://deno.land/x/nanoid@v3.0.0/mod.ts",
    "netsaur": "https://deno.land/x/netsaur@0.2.14/mod.ts",
    "semaphore": "https://deno.land/x/semaphore@v1.1.2/semaphore.ts",
    "terminal_images": "https://deno.land/x/terminal_images@3.1.0/mod.ts",
    "dataframe": "jsr:@sauber/dataframe@^0.0.5",
    "ðŸ“š/": "./src/",
    "ðŸ“¦/": "./web/components/"
  },
  "compilerOptions": { "jsx": "react-jsx", "jsxImportSource": "preact" },
  "exclude": ["**/_fresh/*"]
}
